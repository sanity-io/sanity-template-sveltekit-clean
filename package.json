{
  "name": "sanity-template-sveltekit-clean",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "sveltekit-app",
    "studio"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=sveltekit-app\" \"npm run dev --workspace=studio\"",
    "format": "prettier --cache --write .",
    "postinstall": "npm --workspace sveltekit-app run prepare"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@sanity/eslint-config-studio": "^6.0.0",
    "@sanity/prettier-config": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "3.0.0",
    "prettier-plugin-svelte": "^3.4.1",
    "typescript-eslint": "^8.54.0"
  }
}
